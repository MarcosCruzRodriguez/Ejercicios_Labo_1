#include "biblioteca_utn.h"

//inicializar array con un valor predeterminado
int inicializacionArray(int array[], int size, int valor)
{
	int retorno = 0;

	if(array != NULL && size > 0)
	{
		for(int i = 0; i < size; i++)
		{
			array[i] = valor;
		}
		retorno = 1;
	}
	return retorno;
}

