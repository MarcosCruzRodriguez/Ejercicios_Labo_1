#include <stdio.h>
#include <stdlib.h>
#define LARGO 30
#define CANT_MATERIAS 5


#ifndef DATOS_ALUMNO_H_
#define DATOS_ALUMNO_H_

typedef struct {
	char numero[LARGO];
	int materias[CANT_MATERIAS];
	int sumaNotas;
	float promedio;
}sAlumno;

#endif /* DATOS_ALUMNO_H_ */
