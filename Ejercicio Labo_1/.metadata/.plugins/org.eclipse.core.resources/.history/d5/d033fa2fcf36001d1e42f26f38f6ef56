#include "pedir_numeros.h"

int main(void)
{
	setbuf(stdout, NULL);
	int primerNumero;
	int segundoNumero;

	pedirNumeros(&primerNumero,&segundoNumero);

	printf("_%d\n_%d",primerNumero,segundoNumero);

	return 0;
}
